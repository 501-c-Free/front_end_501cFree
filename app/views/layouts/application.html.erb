<!DOCTYPE html>
<html>
<style>
.choice {
  text-align: center;
}
.title {
  text-align: center;
}
.projects {
  text-align: center;
}
.search {
  text-align: center
}
.social {
  text-align: center
}
</style>
  <head>
    <title>FrontEnd501cfree</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "layouts/nav" %>

    <section class="bg-light">
      <div class="container">
        <div class="row">
          <div class="col-sm-12 pt-4">
            <% flash.each do |type, msg| %>
            <div class="alert alert-info">
              <%= msg %>
            </div>
            <% end %>
          </div>
        </div>
      </div>
    </section>

    <%= yield %>
    <%= render "layouts/footer" %>

  </body>
</html>
