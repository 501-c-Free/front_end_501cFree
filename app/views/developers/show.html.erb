<head>
<% if current_user == @user.user_id%>
  <%= button_to "Edit Profile", developer_edit_path , method: :get %>
<div class="title">
  <h2> <%= "Welcome #{@user.name}" %> </h2>
  <span style='font-size:75px;'>&#128075;</span>
</div>
  <%else %>
<div class="title">
  <h2> <%= "Welcome to #{@user.name}'s Page!" %> </h2>
  <span style='font-size:75px;'>&#128075;</span>
</div>
<%end%>
</head>

<div class="projects">
<div class="social"><a href=<%= @user.linkedin%>><i class="icon ion-social-linkedin"></i></a><a href="#"><i class="icon ion-social-github"></i></a><a href=<%=@user.github %>>
  </div>
  <h4><%= link_to "View All Projects", projects_path %></h4>
  <br>
  <% unless @projects.nil? %>
  <h4>Current Projects</h4>
    <% @projects.each do |project| %>
    <%= link_to(project.project_name, "/project/#{project.project_id}") %>
    
    <%= project.description %>
    <% end %>
  <% end %>
</div>
